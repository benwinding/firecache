{
  "name": "firecache",
  "version": "0.1.0",
  "description": "rxjs state-management for firebase",
  "repository": "https://github.com/benwinding/firecache",
  "homepage": "https://github.com/benwinding/firecache",
  "author": "Ben Winding",
  "license": "MIT",
  "private": false,
  "scripts": {
    "watch": "rimraf dist && yarn watch-tsc",
    "watch-tsc": "concurrently --kill-others \"yarn build:es2015 -w\" \"yarn build:es6 -w\"",
    "build": "rimraf dist && yarn build:es2015 && yarn build:es6",
    "build:es2015": "tsc --outDir dist/lib",
    "build:es6": "tsc --module es6 --target es5 --outDir dist/es6",
    "test": "jest",
    "test:badges": "jest-coverage-badges",
    "clean": "rimraf dist"
  },
  "module": "dist/es6/index.js",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "source": "src/index.ts",
  "peerDependencies": {
    "firebase": "^7.17.0",
    "rxjs": "6.5.x"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "firebase": "7.17.x",
    "jest": "^25.2.1",
    "jest-coverage-badges": "^1.1.2",
    "rimraf": "^3.0.2",
    "rxjs": "6.5.5",
    "ts-jest": "^25.2.1",
    "typescript": "3.5.x",
    "concurrently": "^6.0.2"
  },
  "files": [
    "src",
    "dist",
    "README.md",
    "tsconfig.json"
  ]
}
